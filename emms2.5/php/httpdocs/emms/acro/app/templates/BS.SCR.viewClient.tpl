<h2>{name}</h2>
<table cellspacing=0 cellpadding=0>
<caption><hr></caption>
  <tr>
    <td valign=top>
      <table cellspacing=0 cellpadding=0>
        <tr><td class=label>{status_label}</td><td>{status}</td></tr>
        <tr><td class=label>{nick_label}</td><td>{nick}</td></tr>
        <tr><td class=label>{code_label}</td><td>{code}</td></tr>
        <tr><td class=label>{society_label}</td><td>{society}</td></tr>
        <tr><td class=label></td><td>{society_code}</td></tr>
        <tr><td class=label>{zone_label}</td><td>{zone}</td></tr>
        <tr><td class=label>{advisor_label}</td><td>{advisor}</td></tr>
        <tr><td class=label>{gender_label}</td><td>{gender}</td></tr>
        <tr><td class=label>{birthdate_label}</td><td>{birthdate}</td></tr>
        <tr><td class=label>{cstatus_label}</td><td>{cstatus}</td></tr>
        <tr><td class=label>{spouse_label}</td><td>{spouse}</td></tr>
        <tr><td class=label>{dependants_label}</td><td>{dependants}</td></tr>
        <tr><td class=label>{skills_label}</td><td>{skills}</td></tr>
        <tr><td class=label>{education_label}</td><td>{education}</td></tr>
        <tr><td class=label>{business_label}</td><td class=list>{business}</td></tr>
        <tr><td class=label>{phone_label}</td><td>{phone}</td></tr>
        <tr><td class=label>{mobile_label}</td><td>{mobile}</td></tr>
        <tr><td class=label>{email_label}</td><td>{email}</td></tr>
        <tr><td class=label>{address_label}</td><td>{address}</td></tr>
        <tr><td class=label>{memo_label}</td><td>{memo}</td></tr>
      </table>
    </td>
    <td valign=top><img class=record alt='' src='{img}'></td>
  </tr>
  <tr><td colspan=2><br>{creator}</td></tr>
  <tr><td colspan=2>{editor}</td></tr>
  <tr><td colspan=2>{toolbar}</td></tr>
</table>